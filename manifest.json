{
    "manifest_version": 2,
    "name": "uDownloader",
    "description": "Quick download any youtube video to MP3",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action":
    {
        "default_icon": "icon_19.png",
        "default_popup": "index.html"
    },
    "permissions": ["activeTab"]

}